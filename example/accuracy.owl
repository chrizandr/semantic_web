    <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#hasPart"></rdfs:subPropertyOf>
    <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
