    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://purl.oclc.org/NET/ssnx/ssn</rdfs:isDefinedBy>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is property of</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isQualityOf"></rdfs:subPropertyOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
